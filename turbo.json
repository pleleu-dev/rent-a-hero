{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "storybook": {
      "dependsOn": [
        "^storybook"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "ts:check": {
      "dependsOn": [
        "^ts:check"
      ]
    },
    "lint:check": {
      "dependsOn": [
        "^lint:check"
      ]
    },
    "lint:fix": {
      "dependsOn": [
        "^lint:fix"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "persistent": true,
      "cache": false
    }
  }
}
